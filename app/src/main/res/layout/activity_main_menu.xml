        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headline"
        app:layout_constraintVertical_bias="0.417" />

    <Button
        android:id="@+id/goToFreeStuff"
        android:layout_width="157dp"
        android:layout_height="51dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/limegreen"
        android:elevation="10dp"
        android:text="Gratis Oplevelser"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/goToAddStuff"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headline"
        app:layout_constraintVertical_bias="0.636" />

    <Button
        android:id="@+id/goToList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button til liste test"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="372dp" />
</android.support.constraint.ConstraintLayout>
